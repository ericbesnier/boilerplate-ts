{
  "expo": {
    "name": "boilerplate-ts",
    "slug": "boilerplate-ts",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*",
      "assets/images/*",
      "assets/icons/*",
      "assets/sounds/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deepfocus.boilerplate",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.deepfocus.boilerplate",
      "versionCode": 1
    },
    "description": "boilerplate pour onepoint",
    "githubUrl": "https://github.com/ericbesnier/boilerplate-ts"
  }
}
