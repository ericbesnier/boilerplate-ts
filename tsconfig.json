{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": false,
    "pretty": true
  },
  "typedocOptions": {
    "out": "docs",
    "mode": "modules",
    "exclude": ["node_modules"]
  },
  "files": ["./typings/index.d.ts"]
}
